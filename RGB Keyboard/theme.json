{
  "name": "RGB Keyboard",
  "version": "v6.2",
  "author": "Party Wumpus",
  "manifest_version": 2,
  "inject": {
    "shared.css": [
      "SP"
    ]
  },
  "patches": {
    "BG Colour": {
      "default": "Rainbow",
      "type": "dropdown",
      "values": {
        "Aibow": {
          "BG Colours/aibow.css": [
            "SP"
          ]
        },
        "Rainbow": {
          "BG Colours/rainbow.css": [
            "SP"
          ]
        },
        "Deck Colours": {
          "BG Colours/rgb.css": [
            "SP"
          ]
        },
        "Cotton Candy": {
          "BG Colours/cotton candy.css": [
            "SP"
          ]
        }
      }
    },
    "Key Colour": {
      "default": "Clear Black",
      "type": "dropdown",
      "values": {
        "Clear Black": {
          "Key Colours/clear black.css": [
            "SP"
          ]
        },
        "Black": {
          "Key Colours/black.css": [
            "SP"
          ]
        },
        "Clear White": {
          "Key Colours/clear white.css": [
            "SP"
          ]
        },
        "White": {
          "Key Colours/white.css": [
            "SP"
          ]
        }
      }
    },
    "Animation Speed": {
      "default": "Medium",
      "type": "slider",
      "values": {
        "Fast": {
          "Speeds/fast.css": [
            "SP"
          ]
        },
        "Medium": {
          "Speeds/med.css": [
            "SP"
          ]
        },
        "Slow": {
          "Speeds/slow.css": [
            "SP"
          ]
        },
        "Very Slow": {
          "Speeds/very slow.css": [
            "SP"
          ]
        },
        "Off": {
          "Speeds/off.css": [
            "SP"
          ]
        }
      }
    }
  },
  "id": "36342841-4128-4043-9a56-333e2bc5d170"
}
